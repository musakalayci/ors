Döküm *t 
iş _git 
  Git *imge::kesit::_git, 
  sekme tam, 
  _son  *t8 
{
  İmge := Git->Öz;
  Döküm->hücreAç("git",sekme);
  Döküm->özellikVeKonum(İmge, sekme+2);
  Döküm->hücreAç("gidilen",sekme+2);
  Döküm->İmge(Git->Gidilen, sekme+4, '\n');
  Döküm->kümeKapa(sekme+2, _son);
  Döküm->kümeKapa(sekme, _son);
} 

