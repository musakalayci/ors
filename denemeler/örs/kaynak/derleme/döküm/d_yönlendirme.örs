Döküm *t 
iş _dön 
  İmge *imge::t, 
  sekme tam, 
  _son  *t8 
{
  Döküm->hücreAç("dön",sekme);
  Döküm->özellikVeKonum(İmge, sekme+2);
  Döküm->ifade(İmge->içerik.Dön, "hedef", sekme+2, '\n');
  Döküm->kümeKapa(sekme, _son);
} 





