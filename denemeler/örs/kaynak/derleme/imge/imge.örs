sayaç Sınırlar 
{
  Değişkenler   = 16;
  DiziDerinliği = 16;
  İşlemİsmi     = 256;
}

sayaç Özelleştirme 
{
  Yaban      = 0x1;
  Küresel    = 0x2;
  Bölgesel   = 0x4;
  Yerel      = 0x8;
  Giriş      = 0x10;
  Bünye      = 0x20;
  Sanal      = 0x40;
  Uygulamalı = 0x80;
  Bağımsız   = 0x100;
  Sabit      = 0x200;
  DeğişkenArgüman   = 0x400;
  Sade       = 0x800;
}


ortak içerik 
{
  Genel      şey;
  noktalama  tam; 
  _harf      tam;
  sabitSayı  t64; 
  sayı       simge::sayı; 
  Birim     *kütüphane::t;
  Kesit     *kesit::t;
  
  Tür       *cins::t; 
  Bildiri   *bildiri::t; 
  Dağarcık  *dağarcık::t;
  Değişken  *değişken::t; 
  TürÖzeti  *cins::özet;
  Tekil     *tekil::t;
  Temel     *temel::t; 

  İşlem     *işlem::t; 
  İşlemKonumu *işlem::konum;
  İfade     *imge::t;
  Dahili    *dahil::t; 
  İleti     *_ileti::t;
  Değer     *_değer::t; 
  Çağrı     *çağrı::t;
  Metin     *metin; 
  Harfler   *metin;
  Hazne     *hazne::t; 
  Eğer      *_eğer::t; 
  EğerKi    *_eğer::eğerki;
  Değilse   *_eğer::_değilse;
  Tüm       *_tüm::t;
  Her       *_her::t; 
  Seçim     *_durum::_seçim;
  Durum     *_durum::t; 
  Tekrar    *kesit::içGit;
  Geç       *kesit::içGit;
  DöngüSonu *kesit::içGit;
  İçGit     *kesit::içGit; 
  Git       *kesit::_git; 

  Dön       *imge::t;
  Sil       *imge::t;
  Boşalt    *imge::t;
  Doldur    *imge::ifade::hafıza; 
  Yenile    *imge::ifade::hafıza;
