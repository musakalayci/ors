sayaç Özelleştirme 
{
  Yaban      = 0x1;
  Küresel    = 0x2;
  Bölgesel   = 0x4;
  Yerel      = 0x8;
  Giriş      = 0x10;
  Bünye      = 0x20;
  Sanal      = 0x40;
  Uygulamalı = 0x80;
  Bağımsız   = 0x100;
  Sabit      = 0x200;
  Değişken   = 0x400;
  Sade       = 0x800;
}


ortak içerik 
{
  Genel     şey;
  noktalama tam;
  Birim    *kütüphane::t; 
  Tür      *cins::t; 
  Bildiri  *bildiri::t; 
  Dağarcık *dağarcık::t;
  Değişken *değişken::t; 
  TürÖzeti *cins::özet;
  Tekil    *tekil::t;
  Temel    *temel::t; 
  İşlem    *işlem::t; 
  İfade    *imge::t;
  Dahili   *dahil::t;
}



tür t 
{
  özellik    Özellik;
  konum      simge::konum;
  Ad        *metin;
  Kütüphane *kütüphane::t;
  içerik     içerik; 
  nesne      nesne::t;
}
 
uygulamalı tür sözlük hafıza::küme::k'*t;
uygulamalı tür imgeler hafıza::dizi::k'*t;

küresel iş Yeni Hafıza *hafıza::t, özellik tam: *t 
{
  İmge := <*t>Hafıza->ÖzelYeni(hafıza::Özellik::İmge); 
  İmge->özellik = özellik;
  dön İmge;
}

küresel iş Adlı Hafıza *hafıza::t, Ad *metin, özellik tam: *t 
{
  İmge := <*t>Hafıza->ÖzelYeni(hafıza::Özellik::İmge);
  İmge->Ad = Ad;
  İmge->özellik = özellik;
  dön İmge;
}

