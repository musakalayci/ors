  Hazne->_tüm.Yapılandır("tüm", simge::Terim::Tüm);
  Hazne->_dön.Yapılandır("dön", simge::Terim::Dön);
  Hazne->_sonuç.Yapılandır("sonuç", simge::Terim::Sonuç);

  Hazne->_durum.Yapılandır("durum", simge::Terim::Durum);
