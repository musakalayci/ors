Hazne *hazne 
iş Yapılandır
{
  Hazne->baş.Yapılandır("baş", simge::Özellik::Baş); 
  Hazne->bitiş.Yapılandır("son", simge::Özellik::Son); 
  Hazne->hata.Yapılandır("", simge::Özellik::Hata); 
  Hazne->yorum.Yapılandır("", simge::Özellik::Yorum); 
  Hazne->_ileti.Yapılandır("ileti", simge::Terim::İleti); 

  Hazne->_harf.Yapılandır("", simge::Özellik::Harf); 
  Hazne->sözcük.Yapılandır("", simge::Özellik::Sözcük); 
  Hazne->sözcük1.Yapılandır("", simge::Özellik::Sözcük); 


  Hazne->_metin.Yapılandır("", simge::Özellik::Metin); 
  Hazne->_metin1.Yapılandır("", simge::Özellik::Metin);  

  Hazne->_harfler.Yapılandır("", simge::Özellik::Harfler); 
  Hazne->_harfler1.Yapılandır("", simge::Özellik::Harfler);  


  Hazne->_sayı.Yapılandır("", simge::Özellik::Sayı); 
  Hazne->_sayı1.Yapılandır("", simge::Özellik::Sayı);


  Hazne->üçNokta.Yapılandır("...", simge::Özellik::Üç_Nokta); 
  Hazne->ünlem.Yapılandır("!", simge::Özellik::Ünlem); 
  Hazne->çiftTırnak.Yapılandır("\"", simge::Özellik::Çift_Tırnak); 
  Hazne->kare.Yapılandır("#", simge::Özellik::Kare); 
  Hazne->tersBölü.Yapılandır("\\", simge::Özellik::Bölü_Ters); 
  Hazne->ikiNokta.Yapılandır(":", simge::Özellik::İki_Nokta); 
  Hazne->şapka.Yapılandır("^", simge::Özellik::Şapka); 
  Hazne->yüzde.Yapılandır("%", simge::Özellik::Yüzde); 
  Hazne->t_ve.Yapılandır("&", simge::Özellik::T_Ve); 
  Hazne->tekTırnak.Yapılandır("'", simge::Özellik::Tek_Tırnak); 
  Hazne->parantezAç.Yapılandır("(", simge::Özellik::Parantez_Aç); 
  Hazne->parantezKapa.Yapılandır(")", simge::Özellik::Parantez_Kapa); 
  Hazne->yıldız.Yapılandır("*", simge::Özellik::Yıldız); 
  Hazne->artı.Yapılandır("+", simge::Özellik::Artı); 
  Hazne->virgül.Yapılandır(",", simge::Özellik::Virgül); 
  Hazne->eksi.Yapılandır("-", simge::Özellik::Eksi); 
  Hazne->nokta.Yapılandır(".", simge::Özellik::Nokta); 
  Hazne->soru.Yapılandır("?", simge::Özellik::Soru); 
  Hazne->bölü.Yapılandır("/", simge::Özellik::Bölü); 
  Hazne->çiftNokta.Yapılandır(":", simge::Özellik::İki_Nokta); 
  Hazne->noktalıVirgül.Yapılandır(";", simge::Özellik::Noktalı_Virgül); 
  Hazne->küçüktür.Yapılandır("<", simge::Özellik::Küçüktür); 
  Hazne->eşit.Yapılandır("=", simge::Özellik::Eşit); 
  Hazne->büyüktür.Yapılandır(">", simge::Özellik::Büyüktür); 
  Hazne->kutuAç.Yapılandır("[", simge::Özellik::Kutu_Aç); 
  Hazne->kutuKapa.Yapılandır("]", simge::Özellik::Kutu_Kapa); 
  Hazne->kümeAç.Yapılandır("{", simge::Özellik::Küme_Aç); 
  Hazne->kümeKapa.Yapılandır("}", simge::Özellik::Küme_Kapa); 
  Hazne->bitTersle.Yapılandır("~", simge::Özellik::Bit_Tersle); 
  Hazne->t_veya.Yapılandır("|", simge::Özellik::T_Veya); 
  Hazne->mektup.Yapılandır("@", simge::Özellik::Mektup); 


  Hazne->arttır.Yapılandır("++", simge::Özellik::Arttır); 
  Hazne->azalt.Yapılandır("--", simge::Özellik::Azalt); 
  Hazne->c_veya.Yapılandır("||", simge::Özellik::C_Veya); 
  Hazne->c_ve.Yapılandır("&&", simge::Özellik::C_Ve); 
  Hazne->büyükEşit.Yapılandır(">=", simge::Özellik::Büyük_Eşit); 
  Hazne->küçükEşit.Yapılandır("<=", simge::Özellik::Küçük_Eşit); 
  Hazne->sağaKaydır.Yapılandır(">>", simge::Özellik::Sağa_Kaydır); 
  Hazne->solaKaydır.Yapılandır("<<", simge::Özellik::Sola_Kaydır); 
  Hazne->eşittir.Yapılandır("==", simge::Özellik::Eşittir); 
  Hazne->eşitDeğildir.Yapılandır("!=", simge::Özellik::Eşit_Değildir); 
  Hazne->arama.Yapılandır("::", simge::Özellik::Arama); 
  Hazne->değiştir.Yapılandır("<=>", simge::Özellik::Değiştir); 
  Hazne->bölEşit.Yapılandır("/=", simge::Özellik::Böl_Eşit); 
  Hazne->tVeyaEşit.Yapılandır("^=", simge::Özellik::T_Ters_Veya_Eşit); 
  Hazne->veyaEşit.Yapılandır("|=", simge::Özellik::T_Veya_Eşit); 
  Hazne->veEşit.Yapılandır("&=", simge::Özellik::T_Ve_Eşit); 
  Hazne->çarpEşit.Yapılandır("*=", simge::Özellik::Çarp_Eşit); 
  Hazne->kalanEşit.Yapılandır("%=", simge::Özellik::Kalan_Eşit); 
  Hazne->eksiEşit.Yapılandır("-=", simge::Özellik::Eksi_Eşit); 
  Hazne->artıEşit.Yapılandır("+=", simge::Özellik::Artı_Eşit); 
  Hazne->solaKaydırEşit.Yapılandır("<<=", simge::Özellik::Sola_Kaydır_Eşit); 
  Hazne->sağaKaydırEşit.Yapılandır(">>=", simge::Özellik::Sağa_Kaydır_Eşit); 
  Hazne->bitTersleEşit.Yapılandır("~=", simge::Özellik::Bit_Tersle_Eşit); 
  Hazne->geriOk.Yapılandır("<-", simge::Özellik::Geri_Ok); 
  Hazne->ileriOk.Yapılandır("->", simge::Özellik::İleri_Ok);  
  Hazne->iseOku.Yapılandır("=>", simge::Özellik::İse_Oku); 
  Hazne->pascal.Yapılandır(":=", simge::Özellik::Paskal); 
  
  Hazne->_birim.Yapılandır("birim", simge::Terim::Birim);
  Hazne->_tür.Yapılandır("tür", simge::Terim::Tür);
  Hazne->_ortak.Yapılandır("ortak", simge::Terim::Ortak);
